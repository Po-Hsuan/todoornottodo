if val.delete === "false"
  .card(id=val._id)
    if val.done === "false"
      h1.img
        img(src="../image/x.png" height="50px" width="50px" data-id=val._id).xgoal
    else
      h1.img
        img(src="../image/o.png" height="50px" width="50px" data-id=val._id).ogoal
    a(href=`/goal/${val.goal}`) #{val.goal}
    button.btn.btn-primary.delete(data-id=val._id) delete
